[
  {
    "group": "influxdb",
    "name": "influxdb",
    "className": "org.apache.zeppelin.influxdb.InfluxdbInterpreter",
    "properties": {
      "influxdb.host": {
        "envName": "INFLUXDB_HOST",
        "propertyName": "influxdb.host",
        "defaultValue": "localhost",
        "description": "The host for Influxdb"
      },
      "influxdb.port": {
        "envName": "INFLUXDB_PORT",
        "propertyName": "influxdb.port",
        "defaultValue": "9300",
        "description": "The port for Influxdb"
      },
      "influxdb.cluster.name": {
        "envName": "INFLUXDB_USERNAME",
        "propertyName": "influxdb.username",
        "defaultValue": "root",
        "description": "The userser name for Influxdb"
      },
      "influxdb.result.size": {
        "envName": "INFLUXDB_PASSWORD",
        "propertyName": "influxdb.password",
        "defaultValue": "root",
        "description": "The password name for Influxdb"
      }
    }
  }
]
